<SearchPreset xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>DbConnStringPw</Name>
  <Request>
    <QueryText>AND(OR(NEAR("connectionstring", "password", n=10), NEAR("connstring", "password", n=10), NEAR("data source", "password", n=10)), entity)</QueryText>
    <EnableFql>true</EnableFql>
    <RefinementFilters>filetype:or("config","xml","json","ini","inf","cnf","conf","properties","env","yaml","yml","cs","vb","aspx","php","jsp","jspx")</RefinementFilters>
    <SortList>LastModifiedTime:descending,Rank:descending</SortList>
  </Request>
</SearchPreset>