<SearchPreset xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>DbMgtConfigByName</Name>
  <Request>
    <QueryText>AND(filename:or("phpmyadmin*","adminer*","pgadmin*"), entity)</QueryText>
    <EnableFql>true</EnableFql>
    <RefinementFilters>filetype:or("php","inc","conf","config")</RefinementFilters>
    <SortList>LastModifiedTime:descending,Rank:descending</SortList>
  </Request>
</SearchPreset>