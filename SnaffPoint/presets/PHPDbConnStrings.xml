<SearchPreset xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>PHPDbConnStrings</Name>
  <Request>
    <QueryText>AND(NEAR("mysql_connect", "password", n=30), entity)</QueryText>
    <EnableFql>true</EnableFql>
    <RefinementFilters>filetype:or("php","php3","php4","php5","phtml","inc")</RefinementFilters>
    <SortList>LastModifiedTime:descending,Rank:descending</SortList>
  </Request>
</SearchPreset>
