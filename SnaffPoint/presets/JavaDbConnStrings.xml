<SearchPreset xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>JavaDbConnStrings</Name>
  <Request>
    <QueryText>AND(NEAR("jdbc", "password", n=30), entity)</QueryText>
    <EnableFql>true</EnableFql>
    <RefinementFilters>filetype:or("java","jsp","jspx","properties","xml","conf","config","ini","env")</RefinementFilters>
    <SortList>LastModifiedTime:descending,Rank:descending</SortList>
  </Request>
</SearchPreset>