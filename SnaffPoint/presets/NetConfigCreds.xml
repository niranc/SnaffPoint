<SearchPreset xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>NetConfigCreds</Name>
  <Request>
    <QueryText>AND(OR(NEAR("username", "password", n=10), NEAR("connectionString", "password", n=10), NEAR("connectionString", "pwd", n=10)), entity)</QueryText>
    <EnableFql>true</EnableFql>
    <RefinementFilters>filetype:or("config","xml")</RefinementFilters>
    <SortList>LastModifiedTime:descending,Rank:descending</SortList>
  </Request>
</SearchPreset>