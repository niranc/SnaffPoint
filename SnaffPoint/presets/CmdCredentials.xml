<SearchPreset xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <n>CmdCredentials</n>
  <Request>
    <QueryText>AND(OR(NEAR("runas", "password", n=10), NEAR("net use", "password", n=10), NEAR("cmdkey", "password", n=10)), entity)</QueryText>
    <EnableFql>true</EnableFql>
    <RefinementFilters>filetype:or("bat","cmd","ps1","psm1")</RefinementFilters>
    <SortList>LastModifiedTime:descending,Rank:descending</SortList>
  </Request>
</SearchPreset>