<SearchPreset xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>PsCredentials</Name>
  <Request>
    <QueryText>AND(OR(NEAR("New-Object", "PSCredential", n=10), NEAR("Get-Credential", "password", n=10)), entity)</QueryText>
    <EnableFql>true</EnableFql>
    <RefinementFilters>filetype:or("ps1","psm1","psd1")</RefinementFilters>
    <SortList>LastModifiedTime:descending,Rank:descending</SortList>
  </Request>
</SearchPreset>