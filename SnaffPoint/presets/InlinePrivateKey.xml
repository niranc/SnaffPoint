<SearchPreset xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>InlinePrivateKey</Name>
  <Request>
    <QueryText>AND(OR(NEAR("BEGIN", "PRIVATE KEY", n=2), NEAR("BEGIN", "RSA PRIVATE KEY", n=3), NEAR("BEGIN", "DSA PRIVATE KEY", n=3), NEAR("BEGIN", "EC PRIVATE KEY", n=3), NEAR("BEGIN", "OPENSSH PRIVATE KEY", n=3)), entity)</QueryText>
    <EnableFql>true</EnableFql>
    <RefinementFilters>filetype:or("txt","config","conf","cfg","ini","env","properties","yml","yaml","xml","json","js","py","rb","sh","bash","zsh","ps1","psm1","psd1","cs","php","java","go","pl","pm","t","pl","pm","t","sql","md","log")</RefinementFilters>
    <SortList>LastModifiedTime:descending,Rank:descending</SortList>
  </Request>
</SearchPreset>