    <QueryText>AND(filename:or("web.config","ftpusers","ftpd.conf"), entity)</QueryText>
    <EnableFql>true</EnableFql>
    <SortList>LastModifiedTime:descending,Rank:descending</SortList>
  </Request>
</SearchPreset>